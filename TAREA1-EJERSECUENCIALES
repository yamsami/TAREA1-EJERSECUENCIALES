Funcion ejercicio56(0)
	//Inversión de un arreglo: Invierte el orden de los elementos en un arreglo. Por ejemplo,
	//[1, 2, 3] se convierte en [3, 2, 1].
	//ENTRADA: definicion de variables:
	//arregloOriginal, arregloInvertido, longitudArreglo, i, j
	//PROCESO:
	//pedir al usuario que ingrese los elementos del arreglo
	// i = 1
	// Mientras i <= 10 Hacer
	//Leer arregloOriginal[i]
	//Si arregloOriginal[i] < 0:
	//longitudArreglo = longitudArreglo - 1
	//i = i - 1
	//Si i < 1:
	//i = 1
	//FinSi
	//Sino
	//longitudArreglo = longitudArreglo + 1
	//FinSi
	//Si i >= 10:
	//i = 10
	//FinSi
	//Si longitudArreglo >= 10:
	//longitudArreglo = 10
	//FinSi
	//i = i + 1
	//FinMientras
	//j = longitudArreglo
	//Para i = 1 Hasta longitudArreglo Hacer
	//arregloInvertido[i] = arregloOriginal[j]
	//j =  j - 1
	//FinPara
	//presentar "Arreglo original:"
	//Para i  = 1 Hasta longitudArreglo Hacer
	//presentar arregloOriginal[i]
	//FinPara
	//presentar "Arreglo invertido:"
	//Para i = 1 Hasta longitudArreglo Hacer
	//presentar arregloInvertido[i]
	//FinPara
	//SALIDA:
	//los numeros ingresados se mostraran invertidamente
	Escribir "EJERCICIO 56"
	Definir arregloOriginal, arregloInvertido, longitudArreglo, i, j  Como Entero
	Dimension arregloOriginal[11] , arregloInvertido[11] 
    longitudArreglo = 0
    Escribir "Ingresa los elementos del arreglo (máximo 10 elementos):"
    i = 1
    Mientras i <= 10 Hacer
        Leer arregloOriginal[i]
        Si arregloOriginal[i] < 0 Entonces
            longitudArreglo = longitudArreglo - 1
            i = i - 1
            Si i < 1 Entonces
                i = 1
            FinSi
        Sino
            longitudArreglo = longitudArreglo + 1
        FinSi
        Si i >= 10 Entonces
            i = 10
        FinSi
        Si longitudArreglo >= 10 Entonces
            longitudArreglo = 10
        FinSi
        i = i + 1
    FinMientras
    j = longitudArreglo
    Para i = 1 Hasta longitudArreglo Hacer
        arregloInvertido[i] = arregloOriginal[j]
        j =  j - 1
    FinPara
    Escribir "Arreglo original:"
    Para i  = 1 Hasta longitudArreglo Hacer
        Escribir arregloOriginal[i]
    FinPara
    Escribir "Arreglo invertido:"
    Para i = 1 Hasta longitudArreglo Hacer
        Escribir arregloInvertido[i]
    FinPara
FinFuncion


Funcion ejercicio57(0)
	//Buscar el índice: Pide al usuario que ingrese un valor y encuentra el índice de ese valor
	//en un arreglo. Si el valor aparece más de una vez, muestra todos los índices
	//ENTRADA: definimos variables:
	//n, i, val, x como entero
	//PROCESO:
	//Dimension x[100] 
	//preguntar al usuario cuantos elementos desea ingresar 
	//leer n 
	//pedir al usuario que ingrese los valores
	//para i = 1 hasra n haceer
	//leer x[i]
	//FinPara
	//preguntar al usuario el indice del valor que desea buscar en el arreglo
	//leer val
	//presentar el indice del valor en el arreglo
	//para i = 1 hasta n = Hacer
	//si x[i]=val:
	//presentar i
	//FinSi
	//finpara

	//presentar "el valor no se encuentra en el arreglo
	//SALIDA:
	//segun los numeros ingresados se mostrara el índice del valor en el arreglo
	Escribir "EJERCICIO 57"
	Definir n, i, val, x Como Entero
	Dimension x[100] 
	Escribir "Ingrese la cantidad de elementos a ingresar:"
	Leer n
	Escribir "Ingrese los valores:"
	Para i = 1 Hasta n Hacer
		Leer x[i]
	FinPara
	Escribir "Ingrese el valor a buscar:"
	Leer val
	Escribir "El indice del valor se encuentra en la columna: "
	Para i = 1 Hasta n Hacer
		Si x[i] = val Entonces
			Escribir i
		FinSi
	FinPara
FinFuncion

