Funcion ejercicio14(0)
	//EJERCICIO 14
	//Calculadora simple: Crea una calculadora que realice operaciones básicas como suma, resta, multiplicación 
	// y división, según la elección del usuario.
	// ENTRADA: Definimos variables
	// num1=0?(leer); num2=0?(leer); operación=" "(leer); resul=0?(calcular)
	// PROCESO:
	// Si operación = "+"
	//       resul= num1 + num2
	//SiNo
	//  Si operación = "-" 
	//        resul= num1 - num2
	// SiNo 
	//  Si operación = "*" 
	//        resul= num1 * num2
	// SiNo 
	//  Si operación = "/" 
	//        resul= num1 / num2
	// SiNo 
	//    Escribir "operación inválida"
	// FinSi 
	// SALIDA:
	// Mostrar el resultado en la variable "resul"
	Escribir "EJERCICIO  14"
	Definir num1, num2, resul como Real
	Definir operación Como Caracter
	num1=0; num2=0; operación=" "; resul=0
	Escribir "Ingrese la operación que desea realizar (+, -, *, /): "
	Leer operación
	Escribir "Ingrese número 1: "
	Leer num1 
	Escribir "Ingrese número 2: "
	Leer num2
	Si operación="+" Entonces
		resul= num1 + num2
		Escribir num1, " + ", num2," = ", resul
	SiNo
		Si operación="-" Entonces
			resul= num1 - num2
			Escribir num1, " - ", num2," = ", resul
		SiNo
			Si operación="*" Entonces
				resul= num1 * num2
				Escribir num1, " * ", num2," = ", resul
			SiNo
				Si operación="/" Entonces
					resul= num1 / num2
					Escribir num1, " / ", num2," = ", resul
				SiNo
					Escribir "Operación inválida, ingrese operador (+, -, *, /)" 
				Fin Si
			Fin Si
		Fin Si
	Fin Si
FinFuncion


Funcion ejercicio15(0)
	// EJERCICIO 15
	//Tabla de multiplicar: Pide al usuario un número y muestra su tabla de multiplicar
	// del 1 al 10.
	// ENTRADA: Definimos variables:
	// num=0?(leer); resul=0?(calcular)
	// PROCESO:
	// Si num > 0
	//      Escribir num, " * 1 = ", num * 1
	//      Escribir num, " * 2 = ", num * 2
	//      Escribir num, " x 3 = ", num * 3
	//      Escribir num, " x 4 = ", num * 4
	//      Escribir num, " x 5 = ", num * 5
	//      Escribir num, " * 6 = ", num * 6
	//      Escribir num, " * 7 = ", num * 7
	//      Escribir num, " * 8 = ", num * 8
	//      Escribir num, " * 9 = ", num * 9
	//     Escribir num, " * 10 = ", num * 10
	// SiNo
	//    Escribir "El número ingresado no es válido"
	// FinSi 
	// SALIDA:
	// Mostrar su tabla en la variable "resul"
	Escribir "EJERCICIO  15"
	Definir num, resul Como real
	num=0; resul=0
	Escribir "Ingrese el número para mostrar la tabla de multiplicar del 1 al 10:"
	Leer num
	Si num>0 Entonces
		Escribir "Tabla de multiplicar de ", num ":"
        Escribir num, " x 1 = ", num * 1
        Escribir num, " x 2 = ", num * 2
        Escribir num, " x 3 = ", num * 3
        Escribir num, " x 4 = ", num * 4
        Escribir num, " x 5 = ", num * 5
        Escribir num, " x 6 = ", num * 6
        Escribir num, " x 7 = ", num * 7
        Escribir num, " x 8 = ", num * 8
        Escribir num, " x 9 = ", num * 9
        Escribir num, " x 10 = ", num * 10
	SiNo
		Escribir "El número ingresado no es válido. Debe ser mayor que 0"
	Fin Si
FinFuncion

