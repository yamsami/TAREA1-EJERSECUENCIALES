Funcion ejercicio26(0)
	// Calculadora de precio con descuento: Crea un programa que permita a un usuario ingresar el precio 
	//de un artículo y un porcentaje de descuento. El programa debe calcular y mostrar el precio
	//final después del descuento.
	// ENTRADA: definir variables:
	// prear=0?(leer); pordes=0?(leer); prefin=0?(calcular); pfindes=0?(calcular)
	// PROCESO:
	//prefin= prear * (pordes / 100)
	// pfindes= prear - prefin
	//SALIDA:
	// Presenatar el resultado 
	Escribir "EJERCICIO 26"  
	Definir prear, pordes, prefin, pfindes Como Real
	Escribir "Ingrese el precio del artículo: "
	Leer prear
	escribir "Porcentaje de descuento: "
	Leer pordes
	prefin = prear * (pordes / 100)
	pfindes = prear - prefin
	Escribir "El precio del producto con el descuento es: ", pfindes
FinFuncion

Funcion ejercicio27(0)
	//  Calculadora de factura con impuestos: Solicita al usuario que ingrese el total de una factura y el 
	// porcentaje de impuestos aplicado. Luego, calcula y muestra el monto total a pagar,incluyendo los impuestos.
	// ENTRADA: definicion de variables:
	// tofact=0?(leer); porcentaje=0?(leer); totpaga=0?(calcular); tpagimpu=0?(calcular)
	// PROCESO:
	//  totpagar = tofact * (porcentaje / 100)
	// tpagimpu = tofact + totpagar
	// SALIDA:
	// Presentar "El monto total a pagar, incluyendo impuestos es: "
	Escribir "EJERCICIO 27"
	Definir tofact, porcentaje, totpagar, tpagimpu 	Como Real
	Escribir "ingrese el total de la factura"
	Leer tofact
	Escribir "Ingrese el porcentaje de impuestos aplicados"
	Leer porcentaje
	totpagar = tofact * (porcentaje / 100)
	tpagimpu = tofact + totpagar
	Escribir "El monto total a pagar, incluyendo impuestos es: ", tpagimpu
FinFuncion

Funcion ejercicio28(0)
	// Calculadora de sueldo con aumento: Pide al usuario que ingrese su salario actual y el porcentaje 
	//de aumento que recibirá.
	//Calcula y muestra el nuevo salario después del aumento.
	// ENTRADA:definir variables
	// salarioact=0?(leer); porcentaje=0?(leer); nuevosalario=0?(calcular) nvsalaumento=0?(calcular)
	// PROCESO:
	// nuevosalario = salarioact * (porcentaje / 100)
	// nvsalaumento = salarioact + nuevosalario
	//  SALIDA:
	// Presentar "El nuevo salario después del aumento es: "
	Escribir "EJERCICIO 28"
	Definir salarioact, porcentaje, nuevosalario, nvsalaumento Como Real
	Escribir "Ingrese su salario actual: "
	Leer salarioact
	Escribir "Ingrese el porcentaje de aumento que recibirá: "
	Leer porcentaje
	nuevosalario = salarioact * (porcentaje / 100)
	nvsalaumento = salarioact + nuevosalario
	Escribir "El nuevo salario después del aumento es: ", nvsalaumento
FinFuncion


Funcion ejercicio29(0)
	// Calculadora de compra con múltiples artículos: Permite al usuario ingresar el precio y la cantidad de varios 
	// artículos que está comprando. Calcula el total de la compra y aplica un descuento del 10% si el total es mayor
	// a cierta cantidad (por ejemplo, $100).
	// ENTRADA: definirvariables
	// precio=0?(leer); cantidad=0?(leer); totsindes=0(calcular); totaldes=0?(calcular)
	// PROCESO:
	//"Ingrese el precio del artículo (0 para finalizar): "
	//totsindes = totsindes + (precio * cantidad)
	//FinSi
	// Hasta Que precio <= 0
	//  Si totsindes > 100 Entonces
	// totaldes = totsindes - (totsindes * 0.10)
	// Presentar "Total sin descuento: " 
	// Presejntar "El descuento aplicado es del 10%"
	// Presentar "El descuento aplicado es del 10%"
	// Sino 
	// SALIDA:
	// totaqldes = totsindes
	//  Presentar "total a pagar"
	Escribir "EJERCICIO 29"
	Definir precio, cantidad, totsindes, totaldes Como Real
    totsindes = 0
    Repetir
        Escribir "Ingrese el precio del artículo (ingrese 0 para finalizar): "
        Leer precio
        Si precio > 0 Entonces
            Escribir "Ingrese la cantidad de este artículo: "
            Leer cantidad
            totsindes = totsindes + (precio * cantidad)
        Fin Si
    Hasta Que precio <= 0
	
    Si totsindes > 100 Entonces
        totaldes = totsindes - (totsindes * 0.10)
        Escribir "Total sin descuento: " totsindes
        Escribir "El descuento aplicado es del 10%"
        Escribir "Total con descuento: " totaldes
    Sino
        totaldes = totsindes
        Escribir "Total a pagar: " totaldes 
    Fin Si
FinFuncion


Funcion ejercicio30_31_32(0)
	//30.Calculadora de impuestos sobre el salario: Solicita al usuario que ingrese su salario anual y 
	//calcula el impuesto 
	//sobre la renta según las siguientes tasas:
	//31.De $10,001: 5%
	//32.Más de $20.000: 15%
	//ENTRADA: definir variables:
	// salarioAnual=0.0(LEER); impuesto(LEER)
	//PROCESO:
	// Si salarioAnual <= 10000: 
	//impuesto = salarioAnual * 0.05
	//presentar "El impuesto sobre la renta es:"
	//SiNo
	//FinSi
	//Si salarioAnual >= 10001 y salarioAnual <= 20000:
	//impuesto = (salarioAnual - 10000) * 0.10 + 10000 * 0.05
	//presentar "El impuesto es de:"
	//SiNo
	//impuesto = (salarioAnual - 20000) * 0.15 + 10000 * 0.10 + 10000 * 0.05
	//presentar "El impuesto es de: "
	//finsi
	//SALIDA:
	//segun el salario anual se mostrara el impuesto
	Escribir "EJERCICIO 30-31-32"
	Definir salarioAnual, impuesto Como Real
	Escribir "Ingrese su salario anual: "
	Leer salarioAnual
	Si salarioAnual <= 10000 Entonces
		impuesto = salarioAnual * 0.05
		Escribir "El impuesto sobre la renta es: ", impuesto
	Sino 
	FinSi
		Si salarioAnual >= 10001 y salarioAnual <= 20000 Entonces
			impuesto = (salarioAnual - 10000) * 0.10 + 10000 * 0.05
			Escribir "El impuesto es de: ", impuesto
		Sino
			impuesto = (salarioAnual - 20000) * 0.15 + 10000 * 0.10 + 10000 * 0.05
			Escribir "El impuesto es de: ", impuesto
		FinSi
FinFuncion


